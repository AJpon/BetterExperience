<Window x:Class="BetterGFE.SettingWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BetterGFE"
        mc:Ignorable="d"
        Title="SettingWindow" Height="220" Width="450">
    <Grid>
        <TextBox x:Name="textBox" HorizontalAlignment="Center" Margin="0,48,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120"/>
        <Button x:Name="okButton" Content="OK" HorizontalAlignment="Left" Margin="399,174,0,0" VerticalAlignment="Top" Click="ClickOk" Width="40"/>

    </Grid>
</Window>
